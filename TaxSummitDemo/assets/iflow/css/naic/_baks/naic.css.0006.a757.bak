.selDepFields {
  position: relative;
}

.naics-loading {
  color: #aaa;
}

.nc-nested-item-wrapper {
  margin: 0 0 1em 0;
  padding: 0.5em;
}
.nc-nested-item-wrapper a {
  cursor: pointer;
}
.nc-nested-item-wrapper > .nc-nested-item {
  margin-left: 0.6em;
  margin-right: 0.6em;
}
.nc-nested-item-wrapper > .nc-nested-item:before {
  display: none;
}

.nc-nested-item {
  position: relative;
  margin-left: 1.8em;
  margin-right: 1.8em;
}
.nc-nested-item:before {
  position: absolute;
  top: 1px;
  left: -14px;
  display: block;
  content: "";
  width: 10px;
  height: 14px;
  background: transparent url("../../images/breadcrumb-lshape.png") top center no-repeat;
}
.nc-nested-item a {
  color: #43596b;
  line-height: 1.4em;
  display: inline-block;
}
.nc-nested-item .nc-nested-item {
  margin-top: 0.4em;
}

h3 {
  font-size: 100%;
  margin-left: 1em;
}

@media (min-width: 43em) {
  h3 {
    font-size: 100%;
  }

  .selDepFldsWrapper h3.rBtnWrapper {
    padding-left: 0;
  }

  .selectedRB h3 {
    font-size: 100%;
    padding-left: 0.75em;
    margin-bottom: 0;
  }
}
.nc-results-wrapper h3 {
  font-weight: bold;
}

.selectedRB .fieldWrapper {
  overflow: hidden;
}

fieldset .labelXXS .selDepFldsWrapper {
  width: auto !important;
  margin-right: 1em !important;
}

fieldset .nc-long-desc {
  font-weight: normal !important;
  padding: 0.5em 2.5em 0.5em 2.5em;
  min-height: 2em;
}

.selDepFldsWrapper .fieldWrapper .buttonGroup {
  margin-bottom: 1em;
}

.nc-separator {
  border-top: 1px dotted #aaa;
  width: 98%;
  margin-left: 1%;
}

fieldset div.linePn.labelXXS div.labelPn {
  width: 3%;
}

fieldset .nc-long-desc {
  line-height: 1.2em;
}

.selectedRB .fieldWrapper.not-expanded {
  padding-bottom: 0;
}

a:active,
a:focus,
a:hover {
  text-decoration: none;
}

.group .selDepFldsWrapper .fieldWrapper {
  display: block;
  padding-bottom: 0;
}
.group .selDepFldsWrapper .fieldWrapper.hidden {
  display: none;
}

@media (max-width: 60em) {
  body {
    min-width: auto;
  }

  div#pageContainer {
    margin: 0;
  }
}
.nc-category-wrapper {
  float: none;
  width: 100%;
}

@media (min-width: 43em) {
  .nc-category-wrapper {
    float: left;
    width: 45%;
  }
}
.nc-description-wrapper {
  display: none;
}

@media (min-width: 43em) {
  .nc-description-wrapper {
    display: block;
    float: left;
    clear: none !important;
    width: 54% !important;
    margin: 0 0 1em 0 !important;
  }
  .nc-description-wrapper textarea {
    height: 100%;
  }
}
.searchScope {
  display: none;
}

@media (min-width: 43em) {
  .searchScope {
    display: block;
  }
}
.group .selDepFldsWrapper .fieldWrapper {
  display: block;
}

fieldset .nc-long-desc {
  max-height: auto;
  -webkit-transform: translateZ(0);
  transform: translateZ(0);
  -webkit-transition: max-height 0.15s ease-in;
  transition: max-height 0.15s ease-in;
}
fieldset .nc-long-desc.nc-truncated {
  max-height: 4em;
  overflow-y: hidden;
  -webkit-transform: translateZ(0);
  transform: translateZ(0);
}

fieldset {
  margin-left: 0.7em;
}

.group .selectedRB {
  margin-top: 0 !important;
  margin-right: 1em !important;
  margin-left: 0 !important;
  margin-bottom: 1em !important;
}

.selectedRB .selDepFields {
  border: 1px solid #A7C9D1;
}

.nc-read-more {
  display: block;
  width: 100%;
  text-align: center;
  overflow: hidden;
  background-color: #e0e0e0;
  background-image: -webkit-linear-gradient(top, #f0f0f0, #e0e0e0);
  background-image: linear-gradient(to bottom, #f0f0f0, #e0e0e0);
  background-repeat: repeat-x;
  color: #444;
  cursor: pointer;
  border-top: 1px solid #A7C9D1;
  height: 2em;
  font-weight: normal;
  box-shadow: 0px -1px 10px 3px rgba(122, 147, 153, 0.4);
  -webkit-transition: box-shadow 1s ease-in;
  transition: box-shadow 1s ease-in;
  -webkit-transform: translateZ(0);
  transform: translateZ(0);
}
.nc-read-more:hover {
  background-position: 0 -2em;
  -webkit-transition: background-position 0.1s linear;
  transition: background-position 0.1s linear;
}
.nc-read-more.nc-opened {
  box-shadow: none;
  -webkit-transform: translateZ(0);
  transform: translateZ(0);
}

@media (min-width: 43em) {
  .nc-read-more {
    display: none;
  }
}
@media (max-width: 43em) {
  div.group div.labelXXS div.labelPn,
  div.group div.labelXXS div.dataPn,
  div.group div.labelXS div.labelPn,
  div.group div.labelXS div.dataPn,
  div.group div.labelS div.labelPn,
  div.group div.labelS div.dataPn,
  div.group div.labelM div.labelPn,
  div.group div.labelM div.dataPn,
  div.group div.labelabelMLeft div.labelPn,
  div.group div.labelabelMLeft div.dataPn,
  div.group div.labelabelAuto div.labelPn,
  div.group div.labelabelAuto div.dataPn,
  div.group div.labelL div.labelPn,
  div.group div.labelL div.dataPn,
  div.group div.labelXL div.labelPn,
  div.group div.labelXL div.dataPn,
  div.group div.labelXXL div.labelPn,
  div.group div.labelXXL div.dataPn,
  div.group div.labelXXLLeft div.labelPn,
  div.group div.labelXXLLeft div.dataPn {
    float: none;
    clear: both;
    width: auto;
    margin-top: 0;
    margin-left: 1.5em;
    text-align: left;
  }
  div.group div.labelXXS div.labelPn,
  div.group div.labelXS div.labelPn,
  div.group div.labelS div.labelPn,
  div.group div.labelM div.labelPn,
  div.group div.labelabelMLeft div.labelPn,
  div.group div.labelabelAuto div.labelPn,
  div.group div.labelL div.labelPn,
  div.group div.labelXL div.labelPn,
  div.group div.labelXXL div.labelPn,
  div.group div.labelXXLLeft div.labelPn {
    margin-bottom: 0;
  }
  div.group div.labelXXS div.dataPn,
  div.group div.labelXS div.dataPn,
  div.group div.labelS div.dataPn,
  div.group div.labelM div.dataPn,
  div.group div.labelabelMLeft div.dataPn,
  div.group div.labelabelAuto div.dataPn,
  div.group div.labelL div.dataPn,
  div.group div.labelXL div.dataPn,
  div.group div.labelXXL div.dataPn,
  div.group div.labelXXLLeft div.dataPn {
    margin-bottom: 1em;
  }

  .nc-results-wrapper div.group div.labelXXS div.dataPn,
  .nc-results-wrapper div.group div.labelXS div.dataPn,
  .nc-results-wrapper div.group div.labelS div.dataPn,
  .nc-results-wrapper div.group div.labelM div.dataPn,
  .nc-results-wrapper div.group div.labelabelMLeft div.dataPn,
  .nc-results-wrapper div.group div.labelabelAuto div.dataPn,
  .nc-results-wrapper div.group div.labelL div.dataPn,
  .nc-results-wrapper div.group div.labelXL div.dataPn,
  .nc-results-wrapper div.group div.labelXXL div.dataPn,
  .nc-results-wrapper div.group div.labelXXLLeft div.dataPn {
    width: 90%;
    overflow: hidden;
  }
  .nc-results-wrapper .nc-read-more {
    width: 99%;
  }
}
@media (min-width: 43em) {
  .group .selDepFldsWrapper .fieldWrapper {
    display: none;
  }
  .group .selectedRB {
    margin-top: 1px !important;
    margin-right: 0 !important;
    margin-left: 0 !important;
    margin-bottom: 1px !important;
  }
  .group .nc-category-list div.linePn div.dataPn {
    float: none;
  }

  .selectedRB .selDepFields {
    border-color: transparent;
  }
  .selectedRB .rBtnWrapper {
    border-color: transparent;
    background-color: transparent;
  }
  .selectedRB .fieldWrapper {
    border-top-color: transparent;
    background-color: transparent;
  }

  .selDepFields {
    margin-left: 1em;
    margin-right: 1em;
  }
  .selDepFields h3,
  .selDepFields p.text {
    margin-left: 0;
  }

  fieldset div.linePn.labelXXS div.labelPn {
    width: 1.3em;
  }

  div.group div.labelTop.nc-description-wrapper div.dataPn textarea,
  div.group div.labelXS.nc-description-wrapper div.dataPn textarea {
    width: 98%;
  }

  .selDepFldsWrapper .fieldWrapper .buttonGroup {
    margin-bottom: 1em;
  }

  .nc-results-wrapper h3 {
    font-weight: bold;
    margin-left: 1em;
  }

  .nc-separator {
    margin-top: 1em;
    border-top: 1px dotted #aaa;
    width: 98%;
    margin-left: 1%;
  }
}
