/* Main */
.dp {
    position: absolute;
    font-size: 100%; /* sizes to match page's default font size */
    overflow: visible;
    min-width: 16.5em;
    white-space: nowrap;
}

.dpCalWrap {
    position: relative;
    top: 0;
    left: 0;
    overflow: visible;
    display: inline-block;
    *width: 16.46em;
}

.dpCal {
    position: relative;
    top: 0;
    left: 0;
    border: 1px solid #C6D5E8;
    background-color: #FFF;
    z-index: 2;
}

/* General */
.dp a,
.dp img {
    border: 0;
}

.dp ul {
    list-style: none;
    overflow: visible;
}

.dp ul,
.dp li {
    margin: 0;
    padding: 0;
}

/* Header */
.dpHead {
    position: relative;
    clear: both;
