/* Style initialization adapted from http://meyerweb.com/eric/tools/css/reset/ */
html, body, div, span, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, em, img, strong, tt, fieldset, legend, b, u, i, center, ol, ul, li, form, label, table, tbody, tfoot, thead {
    margin: 0;
    padding: 0;
    border: 0;
    font-size: 100%;
    vertical-align: baseline;
    background: transparent;
}

body {
    line-height: 1em;
}

ol,
ul {
    list-style: none;
}

table {
    border-collapse: collapse;
    border-spacing: 0;
}

tr,
td,
th {
    margin: 0;
    padding: 0;
    border: 0;
    outline: 0;
    font-size: 100%;
    vertical-align: baseline;
}

legend {
    display: inline-block;
    *display: inline;
    *margin-left: -7px;
}

button,
input,
select,
textarea {
    margin: 0;
}

/*
====================================================================================
 Basic XHTML Elements
====================================================================================
*/
html {
    min-height: 100%;
    -webkit-font-smoothing: antialiased;
}

body {
    height: 100%;
    margin-bottom: 15px;
    font-family: "Avenir", "Segoe UI", Helmet, FreeSans, "Helvetica Neue", "Roboto", "Droid Sans", Tahoma, sans-serif;
    font-size: 0.8em;
    background: #CED6E1 url("../images/BodyBg.png") repeat-x scroll left top;
    background-image: -webkit-linear-gradient(#EBF0F4, #CED6E1);
    background-image:         linear-gradient(#EBF0F4, #CED6E1);
    color: #454545;
    text-rendering: optimizeLegibility;
    min-width: 950px;
}

/* needed to prevent footer text from displaying near the top */
form {
    clear: both;
    height: 1%;
    display: inline;
}

table {
    border-collapse: collapse;
}

img,
a img {
    border: 0px;
}

a {
    text-decoration: underline;
    color: blue;
}

input,
select,
textarea {
    font: 100% "Avenir", "Segoe UI", Helmet, FreeSans, "Helvetica Neue", "Roboto", "Droid Sans", Tahoma, sans-serif;
    color: #454545;
    vertical-align: middle;
    padding: 2px 1px 2px 3px;
    margin-top: -3px;
    border: 1px solid #7F9DB9;
    border-radius: 4px;
    background-clip: padding-box;
}

input[type="checkbox"],
input[type="radio"],
input[type="image"] {
    border: none;
}

select {
    margin: 1px 0;
}

div.group div select {
    padding-top: 2px;
    margin-top: 2px;
}

textarea {
    margin-top: 3px;
    resize: vertical;
}

button {
    cursor: pointer;
}

li {
    margin: 0.5em 0 0 1em;
}

p {
    vertical-align: bottom;
}

/* Headings H1-H6 */
h1, h2, h3, h4, h5, h6 {
    font-weight: normal;
}

h1 {
    float: left;
    clear: both;
    margin: 0.5em 0 0.3em 0;
    padding-right: 15px;
    font-size: 160%;
    font-weight: bold;
    line-height: normal;
}

h2 {
    float: none;
    clear: both;
    padding: 7px 15px 7px 0;
    margin: 8px 0 0 0;
    font-size: 140%;
    font-weight: bold;
    line-height: normal;
}

h3 {
    font-size: 140%;
    margin-bottom: 0.5em;
}

h4,
h5 {
    font-size: 140%;
}

h6 {
    font-size: 100%;
}

/*
====================================================================================
 Generic Classes
====================================================================================
*/
.float-left {
    float: left;
}

.float-right {
    float: right;
}

.rt,
.rt input { /* second selector is meant for text inputs inside <td class="rt"> */
    text-align: right;
}

.lt {
    text-align: left;
}

.bold {
    font-weight: bold !important;
}

.notBold {
    font-weight: normal !important;
}

.hidden {
    display: none;
}

.hideOffScreen {
    display: block !important;
    position: absolute !important;
    left: -9999em !important;
    top: -9999em !important;
}

.invisible {
    visibility: hidden;
}

.center {
    text-align: center;
}

.text-top {
    vertical-align: text-top;
}

.noWrap {
    white-space: nowrap;
}

.smallestTextSize {
    font-size: 80%;
    font-weight: normal;
}

.clearPadding {
    padding: 0 !important;
    margin: 0 !important;
}

.noTopMargin {
    margin-top: 0 !important;
}

.numberBullet {
    list-style-type: decimal !important;
}

.removeLink {
    font-weight: normal;
    margin-left: 1.5em;
}

.minWidth {
    width: 1% !important;
}

.expandRows .minWidth {
    /* (CP) In expandable tables `1%` doesn't work in IE7. Likely it's because IE7 requires that the class must be defined in every `<td>`. However expandable tables have "extra" rows (the dynamic ones) with a single `<td colspan="n">` that cannot use this class... */
    *width: 1px !important;
}

.expandRows .selected .minWidth {
    /* ...but when a row is expanded, we have to go back to using a percentage. (CP) */
    *width: 1% !important;
}

.mediumWidth { /* Usually for result sets */
    width: 6em;
}

.noBackgroundImage {
    background-image: none !important;
}

.verticalAlignMiddle {
    vertical-align: middle !important;
}

.italicized {
    font-style: italic;
}

.bullet {
    margin: 0 0.75em;
}

/*
====================================================================================
 Horizontal Partitions
====================================================================================
Notes on c50 and c67:
--------------------
A div with a width of 399px divided into 2 c50 DIVs will by default be 200px each and adding up to a total of 400px.
This happens because the divs 50% width rounds up to the nearest integer and therefore we have to use smaller values.
*/
.c33 {
    width: 33%;
}

.c50 {
    width: 49%;
}

.c100 {
    width: 100%;
}

.c67 {
    width: 66%;
}

.c33 label,
.c50 label,
.c100 label,
.c67 label {
    float: left;
    text-align: right;
}

div.c33 div,
div.c50 div,
div.c67 div,
div.c100 div,
.c33,
.c50,
.c100,
.c67 {
    font-weight: bold;
    float: left;
    margin: 0;
    padding: 0;
    display: inline;
}

.c33 a,
.c50 a,
.c67 a,
.c100 a {
    text-decoration: underline;
    color: #454545;
}

/* The whole page is inside this wrapper div */
#pageContainer {
    margin: 15px 15px 2px 15px;
    background-color: #fff;
    border-color: #9CAEBD #526D84 #526D84 #9CAEBD;
    border-style: solid;
    border-width: 1px;
    padding: 0 0 15px 15px;
    overflow: hidden;
}

.w50 {
    width: 50%;
}

/*
=================================================================================
 Banner
=================================================================================
*/
#bannerContainer {
    margin-left: -15px;
}

#banner, #footer {
    width: 100%;
    position: relative;
    z-index: 0;
    color: #454545;
}

#banner {
    font-size: 0.9em;
}

#bannerMain {
    width: 100%;
    position: relative;
    /* min-height: 136px; *//* This complies with the static's site height - referential only */
    background: #0E3E62 url("../images/BannerBg.png") left top repeat-x;
    background-image: -webkit-linear-gradient(#0F5D80, #0E3E62);
    background-image:         linear-gradient(#0F5D80, #0E3E62);
}

.bannerLeft,
.bannerRight {
    vertical-align: top;
    min-height: 75px;
}

.bannerLeft {
    position: absolute;
    top: 0px;
    left: 0px;
    width: 80px;
}

.bannerRight {
    margin-left: 80px;
    background: transparent url("../images/BannerStatueLiberty.png") top right no-repeat;
}

.bannerBlock {
    display: inline-block;
    *float: left;
    width: 49.5%;
    vertical-align: top;
    padding-top: 0.7em;
}

/* Sub-block sections */
/* Text */
.bannerNYSDTF * {
    color: #FFF;
}

.bannerNYSDTF a {
    text-decoration: none;
}

.bannerNYS {
    margin-top: -0.05em;
    font: italic 1.25em Georgia, "Times New Roman", Times, serif;
}

.bannerDepartment {
    font: 1.85em Georgia, "Times New Roman", Times, serif;
    margin-top: -0.1em;
}

.bannerNames {
    display: inline-block;
    *float: left;
    *clear: both;
    font: 1.17em Georgia, "Times New Roman", Times, serif;
    margin-top: 0.2em;
    padding-top: 0.15em;
    border-top: 1px solid #888;
    white-space: nowrap;
}

.bannerNames a {
    text-decoration: none;
}

.bannerNames a:hover,
.bannerNames a:visited:hover,
.bannerNames a:hover span,
.bannerNames a:visited:hover span {
    text-decoration: underline;
    color: #FC6;
}

.bannerNames a:visited {
    color: #FFF;
}

/* Top Links */
#dtfLinks,
#dtfSearch {
    height: 1%;
    clear: both;
}

#dtfLinks ul {
    float: right;
    list-style-type: none;
}

#dtfLinks ul li {
    float: left;
}

#dtfLinks ul li {
    padding: 0 6px;
    margin: 0;
}

#dtfLinks ul li a {
    color: #FFF;
    text-decoration: none;
    font-size: 1.1em;
    *line-height: 1.25em;
    -webkit-transition: all 0.2s ease;
            transition: all 0.2s ease;
}

#dtfLinks ul li a:hover,
#dtfLinks ul li a:visited:hover {
    text-decoration: underline;
    color: #FC6;
}

#dtfLinks ul li a:visited {
    color: #FFF;
}

#dtfLinks ul li span {
    padding-right: 4px;
}

#footer li.lastItem {
    border: 0;
    padding-right: 0;
}

/* Search */
#dtfSearch {
    padding-top: 1em;
    padding-right: 5px;
}

#dtfSearch label {
    margin: 0 2px;
}

#dtfSearch .searchWrapper {
    float: right;
    position: relative;
}

#dtfSearch .searchWrapper select,
#dtfSearch .searchWrapper input,
.searchBtnIcon {
    margin-top: -3px;
    float: left;
    font-size: 1.1em;
    line-height: normal;
}

#dtfSearch .searchWrapper select {
    border: 0 none;
    margin: 1px 0;
    *margin-top: 0;
    padding: 1px;
    vertical-align: top;
}

#dtfSearch .searchWrapper input {
    border: 0 none;
    border-radius: 0 0 0 0;
    margin-left: 6px;
    margin-top: 3px;
    padding: 0;
}

.textIconCombo,
.searchScope {
    float: left;
    border: 1px solid #036;
    border-radius: 4px;
    background-clip: padding-box;
    background-color: #FFF;
    min-height: 23px;
    line-height: 23px;
}

.searchScope {
    margin-left: 0.5em;
}

.searchBtnIcon {
    background-color: transparent;
    background-image: url("../images/BannerSearchBtnIcon.png");
    background-repeat: no-repeat;
    background-position: center center;
    border: 0 none;
    padding: 2px 0;
    text-indent: 1000em;
    width: 22px;
    height: 22px;
    margin-top: 0;
}

.searchBtnIcon:focus,
.searchBtnIcon:hover {
    background-image: url("../images/BannerSearchBtnIconOver.png");
}

/* 2011 OFT-issued mandatory banner [Start] */
#nysbanner input,
#searchbox,
#searchbutton {
    font: 13px Arial, Helvetica, sans-serif !important;
}

#nysbanner #searchbox {
    margin: 5px 0 0 0 !important;
    *margin: -4px 0 0 0 !important;
    vertical-align: baseline !important;
    *vertical-align: baseline !important;
    padding: 0 !important;
}

#nysbanner #searchbutton {
    margin: 3px 0 0 0 !important;
    *margin: 2px 0 0 0 !important;
    vertical-align: baseline !important;
    padding: 0 3px !important;
}

#nysbanner form {
    clear: none;
}

/* 2011 OFT-issued mandatory banner [End] */

/*
====================================================================================
 Taxpayer Info Box
====================================================================================
*/
/* Using 2-col fluid layout (float: [left, none]) */
#taxpayer-info,
div.formWrapper {
    float: left;
    clear: both;
    width: 100%;
}

div.sectionBorder,
div.mainGroups {
    float: none;
    overflow: hidden;
}

div.sectionBorder {
    height: 1%;
    border: 1px solid #AAA;
    margin-right: 15px;
}

/* See "Labels & Data" CSS for taxpayer-info additional horizontal partitions */
#taxpayer-info div.c33,
#taxpayer-info div.c50,
#taxpayer-info div.c67,
#taxpayer-info div.c100 {
    line-height: 2.0em;
    width: auto;
    margin: 0 4em 0 0;
}

#taxpayer-info div.c100 {
    width: 100%;
}

.hr {
    clear: both;
    height: 2px;
    line-height: 2px;
    margin-right: 15px;
    overflow: hidden;
    border-top: 2px solid #CCC;
    font-size: 1em;
}

/*
=================================================================================
 Top-of-the-Page Navigation, Titles, and Instructions
=================================================================================
*/
#pageHeader {
    float: none;
    display: block;
    width: 100%;
    margin-bottom: 0.3em;
}

/* Main navigation links */
#skipnav {
    position: absolute;
    left: -999px;
    width: 990px;
}

#navHeader {
    display: block;
    float: left;
    clear: both;
    position: relative;
    right: 15px;
    padding-left: 15px;
    width: 100%;
    height: auto;
    list-style: none outside none;
    line-height: 2em;
    white-space: nowrap;
    background: url("../images/MenuNormalBg.png") repeat-x scroll left top #D0D7DE;
    background-image: -webkit-linear-gradient(#F8F9FA, #D2D9DF);
    background-image:         linear-gradient(#F8F9FA, #D2D9DF);
    border-top: 1px solid #9CAEBD;
    border-bottom: 1px solid #9CAEBD;
}

#navHeader ul {
    display: block;
}

#navHeader #navLeft {
    float: left;
    position: relative;
    right: 15px;
}

#navHeader #navRight {
    float: right;
}

#navHeader li {
    float: left;
    margin: 0;
}

#navHeader #navRight a:first-child, /*remove this selector after the new 2011 Framework baseline is adapted */
#navHeader #navRight li:first-child a {
    border-left: 1px solid #B4C1CD;
}

#navHeader a,
#navHeader a:visited,
#navHeader a:link,
#navHeader a:active {
    color: #454545;
    float: left;
    font-size: 110%;
    font-weight: bold;
    border-right: 1px solid #B4C1CD;
    height: 2.25em;
    line-height: 2.25em;
    padding:  0 0.8em;
    text-align: center;
    text-decoration: none;
}

#navHeader a:hover {
    background: url("../images/MenuOverBg.png") repeat-x scroll left top #A0B0C5;
    background-image: -webkit-linear-gradient(#DEE3E9, #A0B0C5);
    background-image:         linear-gradient(#DEE3E9, #A0B0C5);
}

/* prevent double borders in IE */
#navRight a.navHeaderLastLink, /* remove this selector once all menus are rendered with <li> */
#navRight li a.navHeaderFirstLink,
#navRight li a.navHeaderLastLink {
    border-right-width: 0;
}

/*
====================================================================================
 Pre-2011 iflow Banner
====================================================================================
*/
#primaryBanner {
    float: none;
    min-width: 670px;
    height: 60px;
    margin-left: -15px;
    background: #30295A url("../images/headerbg.jpg") top left repeat-x;
    white-space: nowrap;
}

#primaryBanner a,
#primaryBanner img {
    float: left;
    border: none;
}

#logoLeft {
    float: left;
    width: 374px;
    height: 60px;
    white-space: nowrap;
}

/* Force left-size banner's size for now to cover for hybrid apps 05/05/2010 */
#logoLeft img {
    width: 432px !important;
    height: 60px !important;
}

#logoCenter {
    float: none;
    height: 60px;
}

#logoRight {
    float: right;
    width: 293px;
    height: 60px;
    white-space: nowrap;
}

/*
====================================================================================
 [END] Pre-2011 iflow Banner
====================================================================================
*/

#verification {
    display: none;
}

#pageInstructions {
    clear: both;
    padding: 6px 15px 6px 0;
    overflow: hidden;
}

#pageInstructions ul {
    margin-bottom: 0.5em;
}

#legend,
.legend {
    height: 1%;
    font-weight: bold;
}

#legend:after,
.legend:after {
    content: ".";
    clear: both;
    height: 0;
    display: block;
    visibility: hidden;
}

#legend {
    margin-bottom: 1em;
}

#legend ul,
.legend ul {
    float: left;
    list-style-type: none;
}

#legend ul li,
.legend ul li {
    font-size: 90%;
    float: left;
    margin: 0;
    margin-right: 6em;
    line-height: 1.8em;
}

#legend ul li img,
.legend ul li img {
    vertical-align: middle;
    padding-bottom: 3px;
    *padding-bottom: 0px;
}

#legend img.required {
    margin-left: -5px;
}

#legend span.required {
    font-weight: bold;
}

#legend .icon-help {
    /* IE7 needs more space between the icon and text */
    *display: block;
    *float: left;
    *margin-right: 0.2em;
}

div.tablePagingOptions .legend,
div.tablePagingOptions .legend li {
    padding-left: 0px !important;
}

div.tablePagingOptions .legend li {
    font-size: 100% !important;
    font-weight: normal !important;
}

span.errMsg {
    font-weight: bold;
    color: red;
}

p.text {
    margin-bottom: 0.5em;
    line-height: 1.4em;
}

.instructionsSection {
    margin-bottom: 1em;
}

.instructionsSection h3 {
    color: #444;
    font-size: 110%;
    font-weight: bold;
}

/*
=================================================================================
 Error Messages
=================================================================================
*/
#errorMessages {
    float: none;
    clear: both;
    color: #CC0000;
    font-weight: bold;
    width: auto;
    margin: 0 15px 1em 0;
}

#errorMessages ul,
div.formMessages ul {
    overflow: hidden;
}

div.formMessages {
    margin-top: 0;
}

#errorMessages ul li,
ul.formError li {
    float: left;
    clear: both;
    width: 100%;
    list-style-type: none;
    min-height: 14px;
    line-height: normal;
    margin: 0 0 0.25em 0;
    background: transparent url("../images/error.gif") top left no-repeat;
    text-indent: 20px;
}

ul.formError li {
    font-weight: normal;
}

ul li.msgWarning,
ul li.msgInformational,
ul li.msgError,
ul li.msgSuccess,
ul.formError li.msgWarning,
ul.formError li.msgInformational,
ul.formError li.msgError,
ul.formError li.msgSuccess {
    background: none !important;
    text-indent: 0 !important;
}

div.pageMsg ul li p,
ul.formError li p {
    margin: -1px 0 0 20px;
    padding-right: 10px;
    line-height: normal;
}

li.msgWarning,
li.msgInformational {
    color: #454545;
}

li.msgError {
    color: #CC0000;
}

li.msgSuccess {
    color: #009933;
}

.msgIcon {
    background: transparent url("../images/icons_sprite.png") top left no-repeat;
    float: left;
    width: 14px;
    height: 14px;
    overflow: hidden;
    text-indent: -99999em;
}

.formMessages .msgIcon {
    margin-top: 1px;
}

.msgErrorIcon {
    background-position: -16px 0;
}

.msgWarningIcon {
    background-position: 0px 0;
}

.msgInformationalIcon {
    background-position: -32px 0;
}

.msgSuccessIcon {
    background-position: -48px 0;
}

.labelError {
    color: #CC0000 !important;
}

span.customPageMsg {
    display: none;
    clear: both;
    overflow: hidden;
    margin: 2px 0 0.5em 0;
}

/*
=================================================================================
 Page Body & Groups
=================================================================================
*/
div.group {
    float: none;
    height: 1%; /* Three pixel bug */
    padding-bottom: 0.8em;
    line-height: 2.0em;
    vertical-align: middle;
    border: 1px solid #BFBFBF;
    margin: 0 15px 1.25em 0;
}

/* Make sure non-IE browsers wrap their content around the group */
div.group:after {
    content: ".";
    clear: both;
    height: 0;
    visibility: hidden;
    display: block;
}

div.group h4 {
    vertical-align: middle;
    line-height: 1.9em;
    min-height: 1.9em;
    text-indent: 8px;
    border-bottom: 1px solid #BFBFBF;
    background: none repeat scroll 0 0 #E9E9E9;
    font-weight: bold;
    font-size: 110%;
