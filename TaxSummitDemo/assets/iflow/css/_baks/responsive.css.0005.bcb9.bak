/* Unfortunately I have to use these hacks for moneyField. iFlow2 should resolve these HTML issues */
div.moneyField {
    margin-right: 3px; /* Keeps the help icons from being too close */
    *margin-right: 8px; /* IE7 hack */
    width: 12.6em;
    *width: 13.3em; /* IE7 hack */
}

div.group div.moneyField {
    clear: none; /* Needed to overcome the specificity of div.group div */
}

div.moneyField input[size="23"] {
    width: 12.35em;
    width: 12.3em\9;
    *width: 12.95em; /* IE7 hack */
    vertical-align: top\9; /* IE9 hack */
}

div.moneyField input[size="20"] {
    width: 10.95em;
    width: 10.85em\9;
    *width: 11.6em; /* IE7 hack */
    margin-right: -0.25em;
    margin-right: -0.15em\9; /* IE9 hack */
    vertical-align: baseline;
    *vertical-align: middle; /* IE7 hack */
}

@media \0screen { /* IE8 hack */
    div.moneyField {
        width: 13.3em;
    }

    div.moneyField input[size="23"] {
        width: 13em;
        margin-top: 0 !important;
        vertical-align: top;
    }

    div.moneyField input[size="20"] {
        width: 11.55em;
        margin-right: -0.2em;
    }
}

@-moz-document url-prefix() { /* Mozilla hack */
    div.moneyField input[size="23"] {
        margin-top: -0.3em !important;
        width: 12.32em;
    }
    div.moneyField input[size="20"] {
        width: 10.95em;
    }
}

@media all and (-webkit-min-device-pixel-ratio:10000), not all and (-webkit-min-device-pixel-ratio:0) { /* Opera hack */
    div.moneyField input[size="23"] {
        width: 12.32em;
    }
}

/* End of hacks */

