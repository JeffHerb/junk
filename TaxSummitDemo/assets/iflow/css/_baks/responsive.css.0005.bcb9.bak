    width: 2.3em;
    text-align: center;
}

.dpWeekNbr,
.dpDays {
    float: left;
}

.dpWeekNbr {
    background-color: #E7EBF6;
    width: 2.15em;
    text-align: center;
    color: #4C607A;
}

.dpWeekNbr li,
.dpDays a,
.dpDays .disabled {
