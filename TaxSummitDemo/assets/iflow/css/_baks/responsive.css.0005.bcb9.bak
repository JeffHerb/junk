    text-decoration: underline;
}

/* Body */
.dpBody {
    background-color: #FFF;
}

.dpDayDesc,
.dpWeekNbrDays {
    clear: both;
    overflow: hidden;
}

.dpDayDesc {
    background-color: #E7EBF6;
