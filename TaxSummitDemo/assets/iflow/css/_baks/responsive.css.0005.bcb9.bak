.visibleLabel {
    clear: none;
    display: inline-block;
    *display: inline;
    font-weight: normal;
}

.visibleLabel,
.noLabel {
    cursor: default; /* Show the arrow pointer, not the text selection cursor */
}

/* Sizes XXS-S not recommended for sizes M+ */
.labelXXS .selDepFldsWrapper {
    width: 75% !important;
}

.labelXS .selDepFldsWrapper {
    width: 55% !important;
}

.labelS .selDepFldsWrapper {
    width: 35% !important;
}

/* Footer (#footer)
