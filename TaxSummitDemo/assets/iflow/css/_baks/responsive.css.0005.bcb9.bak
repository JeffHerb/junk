/*
==================================================================================
 Ajax
==================================================================================
*/
.ajaxOverlay {
    display: inline-block;
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    overflow: hidden;
    background: transparent url("../images/modalBg.png") top left repeat;
}

.ajaxOverlay img {
    position: absolute;
    top: 44.5%;
    left: 47.5%;
}

/*
==================================================================================
 Copy Buttons
==================================================================================
*/
.copyValue {
    display: block;
    width: 16px;
    height: 16px;
    background: transparent url("../images/icons_sprite.png") -16px -16px no-repeat;
    overflow: hidden;
    text-indent: 1000em;
}

/*
==================================================================================
 Tooltips - Base styles
==================================================================================
*/

.tooltip {
    position: absolute;
    top: 0;
    left: 0;
    float: none;
    max-width: 400px;
    height: auto;
    margin: 0 0.5em 0.5em 0;
    padding: 0.1em;
    outline: none !important;
    border-color: #444;
    border-style: solid;
    border-width: 1px;
    background: #FFFFFF none top left repeat-x scroll;
    box-shadow: 2px 2px 4px #bbb;
    z-index: 2;
}

/* Tooltip pointer fill */
.tooltip:after {
    position: absolute;
    display: block;
    margin-top: 2px;
    content: " ";
    width: 0;
    height: 0;
    z-index: 2;
}

.tooltip:after {
    top: -18px;
    left: 200px;
    border-top: 9px solid transparent;
    border-bottom: 8px solid #ffffff;
    border-left: 9px solid transparent;
    border-right: 9px solid transparent;
}

/* Tooltip pointer border */
.tooltip:before {
    position: absolute;
    display: block;
    margin-top: 2px;
    margin-left: -1px;
    content: " ";
    width: 0;
    height: 0;
    z-index: 1;
}

.tooltip:before {
    top: -19px;
    left: 200px;
    border-top: 9px solid transparent;
    border-bottom: 8px solid #444444;
    border-left: 9px solid transparent;
    border-right: 9px solid transparent;
}

.tooltip .tooltipCloseContainer {
    display: block;
    clear: both;
    margin: 1em;
    padding: 1px;
    text-align: center;
}

.tooltip .tooltipClose,
.tooltip .tooltipClose:hover,
.tooltip .tooltipClose:focus {
    cursor: pointer;
}

.tooltip .tooltipClose:active {
/*outline: none;*/
}

.tooltip .tooltipBody {
    display: block;
    /*float: none;*/
  clear: both;
    padding: 0.4em;
}

.tooltipPointer {
    display: block;
    position: absolute;
    left: -9999em;
    top: -9999em;
    z-index: 3;
}

/*
==================================================================================
 Tooltips - iflow styles
==================================================================================
*/

.tooltip.ttFieldInstructions {
    border-color: #9CAEBD #526D84 #526D84 #9CAEBD;
    background: #FFFFBA url("../images/tooltip_bg.png") top left repeat-x scroll;
    background-image: -webkit-linear-gradient(#FFFFE8, #FFFFBA);
    background-image:         linear-gradient(#FFFFE8, #FFFFBA);
    border-radius: 4px;
    background-clip: padding-box;
}

/* Tooltip pointer fill */
.tooltip.ttFieldInstructions:after {
    border-bottom: 8px solid #FFFFE8;
}

/* Tooltip pointer border */
.tooltip.ttFieldInstructions:before {
    border-bottom: 8px solid #9CAEBD;
}

.tooltip.ttFieldInstructions .tooltipBody h4,
.tooltip.ttFieldInstructions .tooltipBody .topLink {
    display: none !important;
}

.tooltip.ttFieldInstructions p {
    vertical-align: baseline;
}

.tooltip.ttFieldInstructions .tooltipCloseContainer {
    margin: 1em auto;
}

.tooltip.ttFieldInstructions .tooltipClose {
    display: inline-block;
    position: relative;
    top: 4px;
    width: 4em;
    height: 1.85em;
    line-height: 1.75em;
    font-size: 100%;
    border: 1px solid #576F86;
    color: #333;
    text-decoration: none;
    text-align: center;
    background: #DDEAF9 url("../images/ButtonBg.png") repeat-x scroll left top;
    background-image: -webkit-linear-gradient(#FEFEFF 0%, #E9ECF7 44%, #D4DBED 44%, #DFE4F4 100%);
    background-image:         linear-gradient(#FEFEFF 0%, #E9ECF7 44%, #D4DBED 44%, #DFE4F4 100%);
    box-shadow: 1px 1px 2px rgba(0,0,0,0.4);
    border-radius: 4px;
    background-clip: padding-box;
    -webkit-transition: 0.1s ease all;
            transition: 0.1s ease all;
}

.tooltip.ttFieldInstructions .tooltipClose:hover,
.tooltip.ttFieldInstructions .tooltipClose:focus {
    text-decoration: none;
    background: #FEFEFF url("../images/ButtonBgHover.png") repeat-x scroll left top;
    background-image: -webkit-linear-gradient(#FEFEFF 0%, #F0F2FA 44%, #E1E6F3 44%, #E9ECF7 100%);
    background-image:         linear-gradient(#FEFEFF 0%, #F0F2FA 44%, #E1E6F3 44%, #E9ECF7 100%);
    box-shadow: 1px 1px 2px rgba(0,0,0,0.7);
}

.tooltip.ttFieldInstructions .tooltipClose:active {
    text-decoration: none;
    box-shadow: 1px 1px 0 rgba(0,0,0,0.8);
}

.tooltip.ttFieldInstructions .tooltipBody {
    line-height: 1.2em;
    padding: 0.4em;
}

