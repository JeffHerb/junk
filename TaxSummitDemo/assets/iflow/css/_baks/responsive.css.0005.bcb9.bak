}

/* Lists and other */
div.tablePagingOptions p.labelText,
div.tablePagingOptions ul,
div.tablePagingOptions ul li {
    display: inline;
    margin: 0px;
    padding: 0px;
}

div.tablePagingOptions p.labelText {
    padding: 0;
    *padding: 0px 3px; /* IE7 */
}

div.tablePagingOptions div.tablePagingPaneMiddle p.labelText {
    padding: 0px 8px 0px 12px;
    *padding: 0px 12px; /* IE7 */
}

div.tablePagingOptions ul {
    list-style-type: none;
}

div.tablePagingOptions div.tablePagingPaneLeft ul li,
div.tablePagingOptions div.tablePagingPaneMiddle ul li,
div.tablePagingOptions div.tablePagingPaneRight ul li {
    padding: 0px 0px 0px 3px;
    margin: 0em;
    vertical-align: middle;
}

/* Buttons */
.tablePagingOptions input {
    background: transparent url("../images/tablePagingSprite.png") top left no-repeat;
    width: 22px;
    height: 22px;
    margin: auto;
    border: none;
}

.tablePagingOptions input.tablePagingButtonFirst {
    background-position: 0 0;
}

.tablePagingOptions input.tablePagingButtonFirst:hover {
    background-position: 0 -22px;
}

.tablePagingOptions input.tablePagingButtonPrevious {
    background-position: -24px 0;
}

.tablePagingOptions input.tablePagingButtonPrevious:hover {
    background-position: -24px -22px;
}

.tablePagingOptions input.tablePagingButtonNext {
    background-position: -48px 0;
}

.tablePagingOptions input.tablePagingButtonNext:hover {
    background-position: -48px -22px;
}

.tablePagingOptions input.tablePagingButtonLast {
    background-position: -72px 0;
}

.tablePagingOptions input.tablePagingButtonLast:hover {
    background-position: -72px -22px;
}


