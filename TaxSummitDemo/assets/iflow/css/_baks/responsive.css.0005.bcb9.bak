/*
==================================================================================
 File/folder tree table
==================================================================================
*/
.fileTree {
    padding: 1em 0.5em;
    overflow: auto;
    margin-top: 1em;
}

.linePn .fileTree {
    margin-top: 0;
}

.fileTree table {
    width: 100%;
    border: 1px solid #9caebd;
}

.fileTree thead {
    background: url("../images/MenuNormalBg.png") repeat-x scroll left top #d0d7de;
    background-image: -webkit-linear-gradient(#f8f9fA, #d2d9df);
    background-image:         linear-gradient(#f8f9fA, #d2d9df);
}

.fileTree tbody {
    border: 1px solid #9caebd;
    border-top: 1px solid #ccc;
}

.fileTree .folderRow {
    color: black;
}

.fileTree .folderRow:hover {
    cursor: pointer;
}

.fileTree th {
    text-align: left;
    color: #505050;
    font-weight: normal;
    line-height: 1.3em;
    border-top: 1px solid #9caebd;
    border-bottom: 1px solid #ccc;
    padding: 0.25em 1.2em 0.25em 0;
}

.fileTree td {
    padding: 0 14px 0 0;
    margin: 0;
    line-height: 1.75em;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.fileTree tr:last-child td {
    padding-bottom: 0.5em;
}

.fileTree .firstCol {
    width: 25px;
    padding: 2px;
    border-left: 1px solid #9caebd;
}

.fileTree .secondCol {
    min-width: 45%;
}

.fileTree .lastCol {
    text-align: center;
    border-right: 1px solid #9caebd;
}

.fileTree .folderRow td {
    padding-top: 2px;
    padding-bottom: 2px;
}

.fileTree .folderRow .firstCol {
    border-left: 1px solid #9caebd;
}

.fileTree .folderRow .lastCol {
    border-right: 1px solid #9caebd;
}

.fileTree .folderRow .secondCol {
    padding-left: 1px; /* Should match padding in image file, if any */
}

.fileTree .folderRow:hover td {
    background-color: #e9ecf7;
}

.fileTree .fileRow td {
    border-bottom: 1px dashed #ccc;
}

.fileTree .fileRow .firstCol,
.fileTree .fileRow.empty td {
    border-bottom: none;
}

.fileTree .fileCountDisplay {
    display: inline-block;
    margin-left: 0.5em;
    color: #777;
    font-size: 90%;
}

.fileTree .fileTreeIcon {
    background-image: url("../images/fileTree_sprite.png");
    background-position: 0 0;
    display: block;
    float: left;
    position: relative;
    top: 3px;
    left: 0;
    width: 25px;
    height: 16px;
    margin-left: 3px;
}

.fileTree .fileRow .fileTreeIcon {
    width: 16px;
}

.fileTree .folderExpanded .fileTreeIcon {
    background-position: 25px 0;
    *top: -2px;
}

.fileTree .folderCollapsed .fileTreeIcon {
    background-position: 49px 0;
    *top: -2px;
}

.fileTree .empty .secondCol {
    text-indent: 0;
    padding-left: 0;
    background-image: none;
    font-style: italic;
}

.fileTree .button {
    font-size: 90%;
    margin: -0.25em 0.2em 0 0;
    *margin-top: 0;
    padding: 0 0.2em;
    *padding: 0;
    vertical-align: middle;
    height: 1.5em;
    line-height: 1em;
    border: 1px solid #508AAD;
    color: #2E4F63;
}

