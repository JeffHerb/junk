/*
==================================================================================
 Icon Buttons
==================================================================================
*/

.icon-button {
    display: inline-block;
    *display: block;
    overflow: hidden;
    padding: 0;
    vertical-align: middle;
    border: 1px solid transparent;
    background-color: transparent;
    background-repeat: no-repeat;
    background-position: -1px -1px;
    text-indent: 1000em;
    *text-indent: -1000em;
    cursor: pointer;

    /* Override these properties using a class specific to a particular image */
    background-image: none;
    width: 16px;
    height: 16px;
    background-size: 16px 16px;
}

#legend .icon-button {
    *float: left;
}

/* Trash can */
.icon-delete {
    background-image: url("../images/delete.png");
    *width: 18px;
    *height: 20px;
    *background-position: -1px 0;
}

/* Green question mark */
.icon-help {
    background-image: url("../images/info.png");
    *background-position: 0 0;
}

@media (-webkit-min-device-pixel-ratio: 1.5), (min-resolution: 192dppx) {
    .icon-help {
        background-image: url("../images/info@2x.gif");
    }
}

/* Magnifying glass over a document */
.icon-view-details {
    background-image: url("../images/icon-view-details.png"); /* IE7/8 */
    background-image: url("../images/icon-view-details.svg"), none;
    *width: 20px;
    *height: 20px;
    *background-position: 0 0;
}
