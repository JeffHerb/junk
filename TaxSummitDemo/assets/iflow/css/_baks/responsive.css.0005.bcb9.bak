}

.dpHead .fastNavPrevQtr,
.dpHead .navPrevMon,
.dpHead .navNextMon,
.dpHead .fastNavNextQtr {
    border: 1px solid #F1F5FB;
}

.dpHead .fastNavPrevQtr:hover,
.dpHead .navPrevMon:hover,
.dpHead .navNextMon:hover,
.dpHead .fastNavNextQtr:hover {
    border: 1px solid #BBCADB;
    background-color: #DDE6EF;
    border-radius: 3px;
    background-clip: padding-box;
}

.dpHead img {
    *vertical-align: middle;
    padding-top: 2px;
    *padding-top: 0px;
}

.dpHead .title {
    line-height: 1.75em;
