    width: 30%;
}

div.tablePagingOptions div.tablePagingPaneRight {
    width: 30%;
}

div.tablePagingOptions div.tablePagingPaneMiddle {
    width: 39.9%; /* IE7 made me use 39.9% not 40% (rounding bug) */
}

/* Content divs */
div.tablePagingOptions div.tablePagingPaneLeft div {
    padding-left: 6px;
}

div.tablePagingOptions div.tablePagingPaneMiddle div {
    text-align: center;
}

div.tablePagingOptions div.tablePagingPaneRight div {
