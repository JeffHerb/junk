    height: 100%;
}

.dpHead .lIcons {
    left: 0.2em;
}

.dpHead .rIcons {
    right: 0.2em;
}

.dpHead li {
    float: left;
    padding: 0.25em 0.1em 0 0.1em;
}

.dpHead li a,
.dpHead .grayedOut {
