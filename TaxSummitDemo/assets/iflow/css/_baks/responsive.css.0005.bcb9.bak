/*
==================================================================================
 Labels and data - new layout 04/28/2009
==================================================================================
Note:
----
In order to make this transition easier, parent div of class "row" is changing to class "line".
All children elements will keep their names unchanged and inherit the modified CSS from "line".
When cleaning this, look for "row", "line", "required"...
*/
div.group div.line .labelCenterLeft,
div.group div.line .labelLongLeft,
div.group div.line .labelLong,
div.group div.line .labelShort,
div.group div.line .labelCenter,
div.group div.line .labelCenterReq,
div.group div.line .labelCenterNotReq,
div.group div.line .labelTiny {
    float: left;
    position: relative;
    right: 0.9em;
    text-align: right;
    margin: 0.2em;
    margin-left: 1.1em;
}

div.group div.line .labelLong,
div.group div.line .labelCenterReq,
div.group div.line .labelCenterNotReq {
    width: 59%;
}

div.group div.line .labelCenterLeft {
    width: 49%;
}

div.group div.line .labelLongLeft {
    width: 79%;
}

div.group div.line .labelShort {
    width: 39%;
}

div.group div.line .labelCenter {
    width: 49%;
}

div.group div.line .labelTiny {
    width: 9%;
}

div.group div.line .labelAbove {
    clear: both;
    text-indent: 0.5em;
    line-height: 1.25em;
    margin-top: 0.5em;
    width: 100%;
}

div.group div.line span.required {
    position: absolute;
    right: -0.9em;
    padding-bottom: 0.4em;
    line-height: normal;
    color: #CC0000;
}

div.labelAbove span.required {
    position: static !important;
}

/*
==================================================================================
 Labels and data - flexible layout 11/15/2010 (should be final main structures)
==================================================================================
Note:
----
Label classes go now with class=linePn to define children's (labels and data) width
and to give proper layout to these inner sections.
- - - - - - - - - - - - - - - - - - - - - - - - -
.labelTiny = labelXXS
.labelShort = labelS
.labelCenter = labelM
.labelCenterLeft = labelMLeft
.labelLong = labelL
.labelLongLeft = labelXXLLeft
.labelTop = labelAbove
- - - - - - - - - - - - - - - - - - - - - - - - -
*/
/* Width is not 100% bcoz there's no parent container I can give padding to and I don't want to add more XHTML beyond this level */
div.group div.linePn {
    width: 99.2%;
    clear: both;
    position: relative;
    margin: 0em 0.4%;/*overflow: hidden;*/
}

div.group div.labelXXS,
div.group div.labelXS,
div.group div.labelS,
div.group div.labelM,
div.group div.labelMLeft,
div.group div.labelL,
div.group div.labelXXLLeft,
div.group div.labelXL,
div.group div.labelXXL {
    position: relative;
    text-align: right;
    overflow: hidden;
}

/* Auto is like inline */
div.group div.labelAuto {
    width: 100%;
    clear: both;
}

div.group div.labelAuto div.labelPn {
    text-align: left;
    width: auto;
}

/* All other sizes have a fixed size */
div.labelXXS div.labelPn {
    width: 10%;
}

div.labelXXS div.dataPn {
    width: 90%;
    *width: 89.9%;
}

div.labelXS div.labelPn {
    width: 25%;
}

div.labelXS div.dataPn {
    width: 75%;
    *width: 74.9%;
}

div.labelS div.labelPn {
    width: 40%;
}

div.labelS div.dataPn {
    width: 60%;
    *width: 59.9%;
}

div.labelM div.labelPn {
    width: 50%;
}

div.labelM div.dataPn,
div.labelMLeft div.dataPn {
    width: 50%;
    *width: 49.9%;
}

div.labelL div.labelPn {
    width: 60%;
}

div.labelL div.dataPn {
    width: 40%;
    *width: 39.9%;
}

div.labelXL div.labelPn {
    width: 70%;
}

div.labelXL div.dataPn {
    width: 30%;
    *width: 29.9%;
}

div.labelXXL div.labelPn {
    width: 80%;
}

div.labelXXL div.dataPn,
div.labelXXLLeft div.dataPn {
    width: 20%;
    *width: 19.9%;
}

/* Left-aligned labels */
div.labelMLeft div.labelPn {
    width: 48%;
    padding-left: 2%;
    text-align: left !important;
}

div.labelXXLLeft div.labelPn {
    width: 78%;
    padding-left: 2%;
    text-align: left !important;
}

/* linePn immediate children */
div.group div.linePn div.labelPn,
div.group div.linePn div.dataPn {
    float: left;
    clear: none;
    position: relative;
    margin: 0.2em 0;
}

div.group div.linePn div.dataPn {
    text-align: left;
}

div.group div.linePn div.dataPn,
div.group div.linePn div.dataPn input,
div.group div.linePn div.dataPn select {
    font-weight: bold;
}

div.group div.linePn div.dataPn input {
    margin-top: 0;
    margin-top: 2px\9; /* IE9 and lower */
}

div.group div.linePn div.dataPn input[type="text"],
div.group div.linePn div.dataPn input[type="number"],
div.group div.linePn div.dataPn input[type="file"],
div.group div.linePn div.dataPn input[type="password"] {
    margin-bottom: 2px;
}

.rset input[type="text"]:focus,
.rset input[type="number"]:focus,
div.data input[type="text"]:focus,
div.data input[type="number"]:focus,
div.dataPn input[type="text"]:focus,
div.dataPn input[type="number"]:focus,
.rset select:focus,
div.data select:focus,
div.dataPn select:focus,
.rset textarea:focus,
div.data textarea:focus,
div.dataPn textarea:focus {
    background: #fff; /* Ensures that inputs are not gray in striped result sets */
}

/* No spinners on number inputs */
input[type=number]::-webkit-inner-spin-button,
input[type=number]::-webkit-outer-spin-button {
    -webkit-appearance: none;
    margin: 0;
}

div.group div.linePn div.dataPn input.button,
div.group div.linePn div.dataPn label,
div.group div.linePn div.dataPn span.label,
div.group div.linePn div.dataPn span.subLabel,
div.group div.linePn div.dataPn .noLabel {
    font-weight: normal;
}

div.group div.linePn div.clearLineHeight {
    line-height: normal;
    margin-top: 0.4em;
}

/* Buttons within forms have a different look */
div.group .dataPn input.button {
    padding: 0.1em 0.5em;
    margin: 2px 0 1px 0 !important;
    height: 1.7em;
    *line-height: 0.8em;
}

/* Spacing between inputs and adjacent buttons */
div.group div.dataPn select {
    margin-right: 0.25em;
}

/* Inner label */
div.group div.linePn div.labelWrap {
    position: relative;
    margin-right: 1.3em;
    font-weight: normal;
}

div.group div.linePn div.labelWrap span.hidden {
    display: inline-block !important;
    position: absolute;
    left: -9999px;
    top: 0px;
    width: 1px;
    overflow: hidden;
    white-space: nowrap;
    z-index: 0;
}

div.group div.linePn div.labelWrap input[type="radio"],
div.group div.linePn div.labelWrap input[type="checkbox"] {
    margin: 2px -0.3em 0 0;
    *margin: 3px -0.5em 0 0;
    vertical-align: text-top;
}

.labelWrap legend {
    float: right; /* Only needed for IE7/8 and Webkit, but doesn't hurt others (CP 2011-12) */
    *margin: -7px -7px 0 0 !important; /* IE7 only */
}

/* This span is needed to make text align properly in IE7 when the label wraps (CP 2012-01) */
legend .legendWrap {
    display: inline-block; /* Needed for IE8 with Compatibility View, doesn't affect others (CP 2013-09) */
    text-align: right; /* Must be declared again for IE */
}

/* Needed for IE8 with Compatibility View, doesn't affect others (CP 2013-09) */
@media \0screen {
    legend .legendWrap {
        display: block;
        width: 100%;
    }
}

div.group div.linePn div.required {
    position: absolute;
    bottom: 0.1em;
    right: 0.4em;
    color: #CC0000;
    font-weight: normal;
}

/* Label top (above) */
div.group div.labelTop div.labelPn {
    line-height: 1.25em;
}

div.group div.labelTop div.dataPn {
    clear: both;
    margin: 0.2em 0 !important;
    width: 100% !important;
}

div.group div.labelTop div.dataPn textarea {
    width: 99%;
}

.dataPn input[type="radio"] {
    margin-bottom: 3px;
}

.dataPn ul {
     margin-left: 0;
     padding-left: 0;
     *margin-top: -0.3em; /* This compensates for IE7's lack of `:first-child` support, used below */
}

.dataPn ul li {
    line-height: normal;
    margin-left: 0;
    margin-top: 0.25em;
    *margin-top: -0.2em;
}

.dataPn ul li:first-child {
    margin-top: 0.4em;
}

/* Sublabel centered between two input fields (e.g. two dates) */
input + .subLabel.centered,
.calendar + .subLabel.centered {
    margin-left: 0.35em;
    *margin-left: 0.75em;
}

.subLabel.centered + input,
.subLabel.centered + label + input {
    margin-left: 0.7em;
}

/* Center a block that fits its text
   ----------------------- */
.center-block {
    display: block;
    margin: 0 auto;
    text-align: center;
}

.center-block .block-row {
    display: inline-block;
    width: auto;
    text-align: left;
    *display: inline;
    *zoom: 1;
}

.center-block .indented {
    padding-left: 0.3em;
}

.center-block .category-label {
    display: inline-block;
    font-weight: bold;
    text-align: left;
}

/* Selection-dependent fields
   ----------------------- */
.selDepFldsWrapper,
.selectedRB {
    padding: 0;
    margin: 1px 0 !important;
}

.selDepFldsWrapper .selDepFields {
    border: 1px solid #FFF;
}

.rBtnWrapper {
    padding: 0 0 0 0.3em;
    border: 1px solid #FFF;
