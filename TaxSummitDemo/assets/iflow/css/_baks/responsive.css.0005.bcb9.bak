/*
==================================================================================
 Tables
==================================================================================
*/
.rset {
    width: 100%;
    clear: both;
    height: 1%;
    padding: 2px 0px;
    position: relative;
}

.buttonGroup + .rset {
    margin-top: 1em;
}

/* Header */
.rset_head {
    height: 1%;
    padding: 0px 6px;
    overflow: hidden;
    line-height: normal !important;
}

.rset_head div.float-left {
    font-size: 110%;
    font-weight: bold;
}

.rset_head div {
    clear: none;
    padding: 0 !important;
}

.rset_head div.float-right a,
.rset_head div.float-right input {
    float: left;
    margin-left: 0.5em;
}

.rset .legend {
    margin-top: 0.3em;
    font-weight: normal;
}

.rset .expandRowsFunctions {
    overflow: hidden;
    margin-right: 0.3em;
}

.rset .expandRowsFunctions li {
    float: left;
    padding-top: 0.1em;
}

.rset .expandRowsFunctions li a {
    margin-left: 0 !important;
}

.rset_title {
    display: none;
}

/* Body */
.rset_body {
    clear: both;
    height: 1%;
    padding: 2px 6px;
    overflow-x: auto;
    overflow-y: hidden;
}

.rset_body table {
    position: relative;
    width: 100%;
}

.expView table {
    position: static !important;
}

.rset_body thead {
    line-height: 2.4em;
    background: url("../images/TableHeaderBg.png") repeat-x scroll left top #C8DBE4;
    background-image: -webkit-linear-gradient(#FFFFFF 0%, #E0EBF0 42%, #D9E6EC 43%, #C8DBE4 100%);
    background-image:         linear-gradient(#FFFFFF 0%, #E0EBF0 42%, #D9E6EC 43%, #C8DBE4 100%);
}

.rset_body th,
.rset_body td {
    border-width: 1px;
    border-style: solid;
}

.rset_body th {
    padding: 5px 4px;
    line-height: normal;
    text-align: left;
    font-weight: normal;
    vertical-align: top;
    border-color: #AAA;
}

.rset_body th.center {
    text-align: center;
}

.rset_body tbody td {
    padding: 3px 4px;
    line-height: normal;
    font-weight: normal;
    vertical-align: top;
    border-color: #AAA;
}

/* Zebra striping
   Skip tables with an ID containing `nostripes` or the class `.expandRows`,
   and skip rows with class `.ignoreRow` */
.rset_body table:not([id*="nostripes"]):not(.expandRows) tbody tr:nth-child(odd):not(.ignoreRow) td {
    background-color: #FFF;
}

.rset_body table:not([id*="nostripes"]):not(.expandRows) tbody tr:nth-child(even):not(.ignoreRow) td {
    background-color: #F1F1F1;
}

/* JS-based striping classes for older browsers */
.alternateRow,
.alternateRow td {
    background-color: #F1F1F1 !important;
}

.normalRow,
.normalRow td {
    background-color: #FFF !important;
}

/* Grayed-out cells */
.rset_body tbody td.notApplicable {
    background-color: #CCC;
    border-style: solid;
    border-color: #AAA;
    border-width: 0;
    border-bottom: 1px solid #CCC;
}

/* Result set sections */

tr.section-header td,
tr.section-header td a {
    font-weight: bold;
}

tr.section-header,
tr.section-header td {
    background-color: #d4d4d4 !important;
    min-height: 2em;
    line-height: 2em;
}

.rset_body td.indented {
    padding-left: 2em;
}

/* Rset's footer styles */
.rset tfoot {
    width: 100%;
    text-align: right;
}

.rset tfoot td {
    border: none;
    border-top: 3px solid #AAA;
    padding: 0.7em 2px;
    font-weight: bold;
}

.rset tfoot td.rowHeader {
    padding-right: 1em;
}

.rset tfoot td.hasBorder {
    border: 1px solid #AAA;
    border-top: 3px solid #AAA;
}

.rset h5 {
    padding: 0.1em 0;
    font-size: 105%;
    font-weight: bold;
}

.rset input {
    vertical-align: top;
    margin-top: 0;
}

.rset table tr td img {
    vertical-align: middle;
    margin: -3px 0 -2px 0; /* [-3 and -2]... to deal w/browser diffs */
}

.rset .button {
    padding: 0;
    height: 1.7em;
    font-size: 90%;
}

/* if buttons want to display same width, use these accordingly (or add new ones), else don't use them */
.rset .rsetButtonAuto {
    width: auto;
    padding: 0 0.5em !important;
    *padding: 0 !important;
}

.rset .rsetButtonTiny {
    width: 2.7em; /* ~30px */
}

.rset .rsetButtonSmall {
    width: 4.5em; /* ~50px */
}

.rset .rsetButtonMedium {
    width: 6.3em; /* ~70px */
}

.rset .rsetButtonLarge {
    width: 7.8em;
}

.rset .rsetButtonExtraLarge {
    width: 9.3em;
}

.rset .rsetButtonTall {
    padding: 0.1em 0.5em !important;
    height: 1.9em;
    line-height: 1em;
    margin-top: 0.2em !important;
}

.rset .icon {
    cursor: pointer;
}

.rset .tableBtnGroupBelow {
    position: absolute;
    bottom: 1.2em;
    left: 0;
